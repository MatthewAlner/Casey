{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "max-line-length": [true, 180],
    "arrow-parens": [true, "ban-single-arg-parens"]
  },
  "rulesDirectory": []
}